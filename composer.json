{
    "name": "rooberthh/identity",
    "description": "A Laravel package to identify and validate Swedish personal numbers (personnummer) and organization numbers (organisationsnummer).",
    "type": "library",
    "keywords": [
        "laravel",
        "php",
        "personnummer",
        "organisationsnummer",
        "swedish"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roberth Evaldsson",
            "email": "evaldsson.roberth@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/rooberthh/identity/issues"
    },
    "funding": [],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/validation": "^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.13.7",
        "pestphp/pest": "^3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rooberthh\\Identity\\IdentityServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Rooberthh\\Identity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rooberthh\\Identity\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "classmap-authoritative": true
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse --memory-limit=3g"
        ]
    },
    "scripts-descriptions": {
        "pint": "Run Laravel Pint on the codebase.",
        "stan": "Run PhpStan on the codebase."
    }
}
